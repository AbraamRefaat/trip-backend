{
    "functions": {
        "api/*.js": {
            "maxDuration": 60,
            "memory": 1024
        }
    },
    "rewrites": [
        {
            "source": "/",
            "destination": "/api/index.js"
        },
        {
            "source": "/api/trips",
            "destination": "/api/trips.js"
        }
    ]
}